    "import numpy as np\n",
    "import matplotlib.pylab as plt\n",
    "\n",
    "def relu_function(x):\n",
    "    return np.where(x<=0,0,x)\n",
    "x=np.linspace(-5,5)\n",
    "y=relu_function(x)\n",
    "\n",
    "plt.plot(x,y)\n",
    "plt.xlabel(\"x\")\n",
    "plt.ylabel(\"y\")\n",
    "plt.show()"
   ]
